module Google
  module Cloud
    module Firestore
      class DocumentSnapshot
        def data: () -> Hash[Symbol, untyped]?
        def exists?: () -> bool
      end

      class DocumentReference
        def set: (Hash[untyped, untyped] data, ?merge: untyped?) -> void
        def update: (Hash[untyped, untyped] data, ?update_time: Time?) -> void
        def get: () -> DocumentSnapshot
        def delete: (?exists: bool?, ?update_time: Time?) -> untyped
      end

      def self.new: (?database_id: string?) -> Firestore

      def doc: (String document_path) -> DocumentReference
    end
  end
end
